package javaprograms;

public class DuplicareCharcount {
	
	
	public static void getCharactercont(String str1,char v)
	{
		
	String str="suresh";
	int count=0;
	for(int i=0;i<=str.length();i++)
	{
		if(str.charAt(i)==v)
		{
			count++;
			System.out.println("The number of displayof s character is:"+ count);
		}
	}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		getCharactercont("suresh",'s');
		

	}

}
